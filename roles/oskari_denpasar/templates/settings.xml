<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">
  <mirrors>
    <mirror>
      <id>oskari_org</id>
      <name>Oskari.org release repository</name>
      <url>https://oskari.org/repository/maven/releases/</url>
      <mirrorOf>oskari_org</mirrorOf>
    </mirror>
    <mirror>
      <id>central</id>
      <name>central</name>
      <url>https://repo.maven.org/maven2</url>
      <mirrorOf>central.maven.org</mirrorOf>
    </mirror>
    <mirror>
      <id>osgeo-release</id>
      <name>OSGeo Repository</name>
      <url>https://repo.osgeo.org/repository/release/</url>
      <mirrorOf>osgeo</mirrorOf>
    </mirror>
    <mirror>
      <id>geoserver-releases</id>
      <name>Boundless Repository</name>
      <url>https://repo.osgeo.org/repository/Geoserver-releases/</url>
      <mirrorOf>boundless</mirrorOf>
    </mirror>
  </mirrors>
</settings>
